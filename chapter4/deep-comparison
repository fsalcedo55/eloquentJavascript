function deepEqual(firstValue, secondValue) {
  if (
    typeof firstValue == "object" &&
    typeof secondValue == "object" &&
    typeof firstValue != null &&
    typeof secondValue != null
  ) {
    if (Object.keys(firstValue).length != Object.keys(secondValue).length) {
      return firstValue === secondValue;
    } else {
      for (let property in firstValue) {
        if (!property in secondValue) {
          return false;
        } else if (firstValue.property != Object.keys(secondValue)) return true;
      }
    }
  } else return firstValue === secondValue;
}

let obj = { here: { is: "an" }, object: 2 };
// deepEqual(obj, obj)
// → true
deepEqual(obj, { here: 1, object: 2 });
// → false
deepEqual(obj, { here: { is: "an" }, object: 2 });
// → true
